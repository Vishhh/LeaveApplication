<div class ="col-md-10">
	<table class="display table table-bordered table-striped">
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>Salary</th>
			</tr>
		</thead> 
		<tbody>
			<% @employees.each do |employee| %>
			<tr>
				<td><%= employee.name %></td>
				<td><%= employee.email %></td>
				<td><%= employee.salary %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>
