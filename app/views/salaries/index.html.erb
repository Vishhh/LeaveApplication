<% if current_user %>

<div class="row">
  <div class="col-lg-10">
    <div class="row">
      <div class="col-lg-10" id="example_filter">
        <div class="row search-row">
          <div class="col-md-6 pull-left">
 <%= link_to 'New Salary', new_salary_path, { :class => 'btn btn-success' } %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class ="col-md-10">
	<table class="display table table-bordered table-striped">
		<thead>
			<tr>
				<th>Month</th>
				<th>Year</th>
				<th>Working Days</th>
			</tr>
		</thead> 
		<tbody>
			<% @salaries.each do |salary| %>
			<tr>
				<td><%= salary.month %></td>
				<td><%= salary.year %></td>
				<td><%= salary.working_days %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>
<%end%>